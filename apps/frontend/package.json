{
  "name": "forza-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 5000 -H 0.0.0.0",
    "build": "next build --no-lint",
    "start": "next start -p 5000 -H 0.0.0.0",
    "lint": "next lint --max-warnings=0",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true next build",
    "playwright:install": "playwright install chromium",
    "seed:test-users": "tsx scripts/seed-test-users.ts",
    "test:e2e:auth": "playwright test --grep auth",
    "docs:update": "tsx scripts/update-docs.ts",
    "gen-types": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > lib/supabase/generated.types.ts",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.75.0",
    "autoprefixer": "^10.4.20",
    "lucide-react": "^0.469.0",
    "next": "^15.5.4",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "swr": "^2.2.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.56.1",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.5.4",
    "prettier": "^3.3.3",
    "tsx": "^4.20.6"
  }
}
