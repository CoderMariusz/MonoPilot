{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "installCommand": "npm install --legacy-peer-deps && cd apps/frontend && npm install --legacy-peer-deps",
  "buildCommand": "cd apps/frontend && npm run build",
  "outputDirectory": "apps/frontend/.next",
  "crons": [{
    "path": "/api/cron/cleanup-invitations",
    "schedule": "0 2 * * 0"
  }]
}
